{
  "compilerOptions": {
      "target": "ES2016",
      "module": "commonjs",
      "outDir": "./build",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "strictPropertyInitialization": false,
      "paths": {
        "*": ["./src/types/*", "./node_modules/@types"]
      }
  },
  "include": ["src/**/*"]
}